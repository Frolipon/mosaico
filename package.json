  "scripts": {
    "deps": "export NODE_ENV=development && npm install && bower-npm-install --non-interactive",
    "test": "grunt jasmine_node",
    "bower": "bower-npm-install --non-interactive",
    "build": "npm run deps && gulp build --prod",
    "build-dev": "npm run deps && gulp build",
    "start": "export NODE_ENV=production && node server/workers.js",
    "prod": "npm run build && npm start",
    "dev": "gulp dev --watch",
    "release": "./bin/dist.sh",
    "deploy": "npm run release",
    "build-release-dev": "npm run build-dev && gulp bump",
    "build-release": "npm run build && gulp bump",
    "sync-db": "node ./bin/db-sync.js",
    "backup-db": "node ./bin/db-backup.js",
    "local-db": "node ./bin/db-manage-local.js",
    "db-test": "node ./bin/db-switch-to-data-test.js --set",
    "db-test-teardown": "node ./bin/db-switch-to-data-test.js --restore",
    "tape": "tape './tests/functional/*.js' | faucet",
    "test-back": "npm run tape",
    "consolidate-creations": "node ./bin/db-consolidate-creations.js",
    "toc": "gulp toc"
  },
